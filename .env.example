# Copy this file to .env and fill in values for your environment

# Application
NODE_ENV=development
PORT=3000
APP_NAME=
APP_SLUG=
APP_HOST=
APP_URL=http://localhost:3000
APP_PREFIX=
APP_VERSION=
APP_DESCRIPTION=

# Database (example: Postgres)
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=postgres
DATABASE_NAME=nest_starter
DATABASE_URL=postgres://postgres:postgres@localhost:5432/nest_starter

# JWT
JWT_SECRET=change_me_secure_random
JWT_EXPIRES_IN=3600s

# Redis (optional)
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=info

# Other
SENTRY_DSN=